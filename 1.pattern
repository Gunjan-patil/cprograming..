#include <stdio.h>
#include <conio.h>

int main()
{
  int i=0,j=0,c=0,Num=15,Temp=5;
  
  printf("Enter a Value for Pattern=");
  scanf("%d",&c);
   
  for(i=1;i<=c;i++)
  {
    for(j=1;j<=c;j++)
    {
      if(i>=j)
      {
        
      printf(" %3d ",Num);
      Num=Num+Temp;
      
      }
      else
      {
        printf(" ");
      }
    }
    
    printf("\n");
    
  }
  return 0;
  
